<?xml version="1.0" encoding="UTF-8" ?>
<derivedformatmanagement xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DerivedFormatDeclarative.xsd">
    <!-- first official version for R2023x Beta2 and R2022x FD04  -->
	<converter name="CREO" synchronous="true"  nlsFile="emxDerivedFormatManagementCREOStringResource" comment="conversion managed synchronously at save by CREO PowerBy connector">
      <datasources>
		<datasource name="CREO" >    
			<source name="PRT,XPR" type = "Part" >
				<target name="image_jpg" inputStreamId="authoring" outputStreamId="JPG"  downloadable="true" />
				<target name="tif"       inputStreamId="authoring" outputStreamId="TIF"  downloadable="true" />
				<target name="iges"      inputStreamId="authoring" outputStreamId="IGES" downloadable="true" />
				<target name="png"       inputStreamId="authoring" outputStreamId="PNG"  downloadable="true" />
				<target name="step"      inputStreamId="authoring" outputStreamId="STEP" downloadable="true" />
			</source>
					
			<source name="DRW" type ="Drawing">
				<target name="image_jpg" inputStreamId="authoring" outputStreamId="JPG" downloadable="true" />
				<target name="postscript" inputStreamId="authoring" outputStreamId="PS" downloadable="true" />
				<target name="dxf"      inputStreamId="authoring" outputStreamId="DXF"  downloadable="true" />
				<target name="iges"     inputStreamId="authoring" outputStreamId="IGES" downloadable="true" />
				<target name="cgm"      inputStreamId="authoring" outputStreamId="CGM"  downloadable="true" />				
				<target name="pdf"      inputStreamId="authoring" outputStreamId="PDF"  downloadable="true" />
				<target name="dwg"      inputStreamId="authoring" outputStreamId="DWG"  downloadable="true" />
			</source>
			
			<events>       
				<event name="onXCADSave" type="interactiveUpdate" />
			</events>
		</datasource>
	  </datasources>
	</converter>

</derivedformatmanagement>
