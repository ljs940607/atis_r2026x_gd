<?xml version="1.0" encoding="utf-8" ?>

<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template syp:name="3DPlayDMUExperienceHeaders" Target="CATCmdHeadersList">
    <CATCmdHeadersList>
      <!--3DPlay command headers-->
      <CATCommandHeader ID="DMUPublishToSwymCmdHdr" ClassName="DS/DMUBaseCommands/DMUPublishToSwymCmd" HelpResourceFile = "DMUBaseCommands/DMUBaseCommands" ResourceFile="DMUBaseCommands/DMUBaseWebCommands" />
      <CATCommandHeader ID="SharePrint" Available="0" ClassName="DS/3DPlayCommands/CmdSharePrint" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands" ResourceFile="3DPlay/3DPlay" />
      <CATCommandHeader ID="ShareDownload" Available="0" ClassName="DS/3DPlayCommands/CmdShareDownload" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands" ResourceFile="3DPlay/3DPlay" />
      <CATCommandHeader ID="CATWebUXSlideShowHdr" ClassName="DS/CATWebUXSlideShow/CATWebUXSlideShowCmd" ResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" HelpResourceFile="CATWebUXSlideShow/CATWebUXSlideShow" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ControllerID" Value="DMU3DMarkupSlideShow" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID = "VisuZoomView"                         ClassName = "DS/ViewerCommands/VisuZoomViewCmd"                                   ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" />
      <CATCommandHeader ID = "VisuPanView"                          ClassName = "DS/ViewerCommands/VisuPanViewCmd"                                    ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" />
      <CATCommandHeader ID = "VisuRotateView"                       ClassName = "DS/ViewerCommands/VisuRotateViewCmd"                                 ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" />
      <CATCommandHeader ID = "VisuReframeView"                      ClassName = "DS/ViewerCommands/VisuReframeViewCmd"                                ResourceFile = "ViewerCommands/ViewerCommands"                 HelpResourceFile = "ViewerCommands/ViewerCommandsIDs" />

      <CATCommandHeader ID="VisuBasicOCACmd" ClassName="DS/ViewerCommands/VisuBasicOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuBlueDesignOCACmd" ClassName="DS/ViewerCommands/VisuBlueDesignOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuCityOCACmd" ClassName="DS/ViewerCommands/VisuCityOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuDarkDesignOCACmd" ClassName="DS/ViewerCommands/VisuDarkDesignOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuDarkMirrorOCACmd" ClassName="DS/ViewerCommands/VisuDarkMirrorOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuDarkReviewOCACmd" ClassName="DS/ViewerCommands/VisuDarkReviewOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuIndoorOCACmd" ClassName="DS/ViewerCommands/VisuIndoorOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuOutdoorOCACmd" ClassName="DS/ViewerCommands/VisuOutdoorOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuPureWhiteOCACmd" ClassName="DS/ViewerCommands/VisuPureWhiteOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuRoadOCACmd" ClassName="DS/ViewerCommands/VisuRoadOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuStudioOCACmd" ClassName="DS/ViewerCommands/VisuStudioOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuWhiteDesignOCACmd" ClassName="DS/ViewerCommands/VisuWhiteDesignOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuWhiteMirrorOCACmd" ClassName="DS/ViewerCommands/VisuWhiteMirrorOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuWhiteReviewOCACmd" ClassName="DS/ViewerCommands/VisuWhiteReviewOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuWhiteExperienceOCACmd" ClassName="DS/ViewerCommands/VisuWhiteExperienceOCACmd" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuQMCommand" ClassName="DS/ViewerCommands/VisuQMCommand" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile = "ViewerCommands/ViewerCommandsIDs"/>
      <CATCommandHeader ID="VisuViewPanelCommand" ClassName="DS/ViewerCommands/VisuViewPanelCommand" ResourceFile="ViewerCommands/ViewerCommands" HelpResourceFile="ViewerCommands/ViewerCommandsIDs" />
      <CATCommandHeader ID="PAD3DToggleStatusBarHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DToggleStatusBarCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" HelpResourceFile ="ENOPAD3DViewer/ENOPAD3DViewerHelpForCommands" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="PAD3DToggleSimplifiedHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerToggleGeometry" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="mode" Value="optimized" Mode="In"/>
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="PAD3DViewerPrintHdr" ClassName="DS/3DPlayCommands/CmdSharePrint" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" HelpResourceFile="ENOPAD3DViewer/ENOPAD3DViewerHelpForCommands"/>
      <CATCommandHeader ID="ViewerScreenshot" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerScreenshotCmd"          HelpResourceFile = "ENOPAD3DViewer/ENOPAD3DViewerHelpForCommands" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="appPrefix" Value="3DPlay" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="DMUCaptureCmdHdr" ClassName="DS/CATWebUXCommands/DMUCaptureCmd" ResourceFile="CATWebUXCommands/CATWebUXCommands"      HelpResourceFile = "CATWebUXCommands/CATWebUXCommands"/>
      <CATCommandHeader ID="PAD3DToggleAccurateHdr" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerToggleGeometry" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="mode" Value="high" Mode="In"/>
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID = "ViewSelector"                         ClassName = "DS/3DPlayCommands/CmdViewSelector"                                   ResourceFile = "3DPlay/3DPlayExperience3D"                     HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />
      <CATCommandHeader ID="FlyWalkCmd" ClassName="DS/FlyWalkCommand/FlyWalkCmd" HelpResourceFile="FlyWalkCommand/FlyWalkCommandIDs" ResourceFile="FlyWalkCommand/FlyWalkCmd" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ID="DMUSwitch3DMarkersResponsiveDisplayHdr" ClassName="DS/DMUBaseCommands/DMUSwitch3DMarkersResponsiveDisplayCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands" HelpResourceFile="DMUBaseCommands/DMUBaseCommands"/>

      <CATCommandHeader ClassName="DS/DMUBaseCommands/DMURightSidePanelCmd" ID="RightSidePanel" HelpResourceFile="PADUtils/PADUtilsHelpForCommands" ResourceFile="PADUtils/PADUtils" Available="1" Behavior="CATAfrHeaderCheck">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="PropertiesReadOnly" Value="true" Mode="In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID ="measure2CmdHdr" ClassName="DS/3DPlayCommands/Proxy/CmdMeasureProxy" HelpResourceFile="DMUMeasure/DMUMeasure" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dPlay" Value = "3dPlay" Mode = "In" />
          <CATCommandHeaderArg ID = "RemoveNonPersistent" Value = "false" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>

      <CATCommandHeader ID="section2CmdHdr" ClassName="DS/3DPlayCommands/Proxy/CmdSectionProxy" HelpResourceFile="DMUSection/DMUSection" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID = "3dReview" Value = "3dReview" Mode = "In" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="LockUnlockSectionHdr" ClassName="DS/DMUSection/DMULockUnlockSection" ResourceFile="DMUBaseCommands/3DPlayPro" HelpResourceFile="DMUSection/DMUSection" Behavior="CATAfrHeaderCheck" />
      <CATCommandHeader ID="DMUPrintHdr" ClassName="DS/DMUPersistence/DMUPrintCmd" ResourceFile="DMUPersistence/DMUPersistence" HelpResourceFile="DMUPersistence/DMUPersistence" />
      <CATCommandHeader ID="CATWebUXPreferencesHdr" ClassName="DS/CATWebUXPreferences/CATWebUXPreferencesCmd" ResourceFile="CATWebUXPreferences/CATWebUXPreferences"    HelpResourceFile = "CATWebUXPreferences/CATWebUXPreferences" />
      <CATCommandHeader ID="CAT3DAnnotationBlankingHdr" ClassName="DS/CAT3DAnnotationBaseCmds/CAT3DAnnotationBlankingCmd" ResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" HelpResourceFile="CAT3DAnnotationBaseCmds/CAT3DAnnotationBaseCmds" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ID="DMUCopyLinkHdr" ClassName="DS/DMUBaseCommands/DMUCopyLinkCmd" ResourceFile="DMUBaseCommands/DMUBaseWebCommands"  HelpResourceFile = "DMUBaseCommands/DMUBaseCommands" />
      <CATCommandHeader ID ="VisibilityCommands" ClassName = "DS/3DPlayCommands/VisibilityCommands" ResourceFile = "3DPlay/3DPlayExperience3D" HelpResourceFile = "3DPlayCommands/3DPlayHelpForCommands3D" />
      <CATCommandHeader ID="DMUEditPropertiesHdr" ClassName="DS/DMUCommands/DMUEditPropertiesCmd" ResourceFile="DMUCommands/DMUCommands" HelpResourceFile = "DMUCommands/DMUCommands">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg readOnly="true" />
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
    </CATCmdHeadersList>
  </Template>

  <Template syp:name="3DPlayDMUExperienceAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="3DPlayDMUExperienceWorkbench">

      <CATCmdContainer Name="Fixed Area" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="CATWebUXSlideShowHdrStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdSeparator Name = "VisuMode_Separator"/>
        <!-- <CATCmdContainer Name = "Share" Visibility = "1" RepresentationType="AfrActionBarDropDown"> -->
        <CATCmdStarter Name = "DMUPublishToSwymCmd"  Command = "DMUPublishToSwymCmdHdr" />
        <!-- <CATCmdStarter Name = "ShareDownload"  Command = "ShareDownload" />
          <CATCmdStarter Name = "SharePrint"  Command = "SharePrint" /> -->
        <!-- </CATCmdContainer> -->
      </CATCmdContainer>

      <CATCmdContainer Name = "SectionViews" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="DMUSwitch3DMarkersResponsiveDisplayStr" Command="DMUSwitch3DMarkersResponsiveDisplayHdr" />
        <CATCmdStarter Name="CAT3DAnnotationBlankingStr" Command="CAT3DAnnotationBlankingHdr" />
        <CATCmdSeparator Name="Separator1"/>
        <CATCmdStarter Name = "VisuReframeViewStr"       Command = "VisuReframeView" />
        <CATCmdStarter Name = "VisuPanViewStr"           Command = "VisuPanView" />
        <CATCmdStarter Name = "VisuRotateViewStr"        Command = "VisuRotateView" />
        <CATCmdStarter Name = "VisuZoomViewStr"          Command = "VisuZoomView" />
        <CATCmdStarter Name = "ViewSelector"            Command = "ViewSelector" />
        <CATCmdStarter Name="VisuViewPanelCommand" Command="VisuViewPanelCommand" />
        <CATCmdSeparator Name="RSP_Separator"/>
        <CATCmdStarter Name = "FlyWalkStr" Command="FlyWalkCmd"/>
        <CATCmdSeparator Name="RSP_Separator2"/>
        <CATCmdStarter Name="RightSidePanel" Command="RightSidePanel" />
      </CATCmdContainer>

      <CATCmdContainer Name = "Tools" Visibility = "1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name = "VisibilityCommands"  Command = "VisibilityCommands" />
        <CATCmdStarter Name = "Measure"                 Command = "measure2CmdHdr" />
        <CATCmdContainer Name = "Section" Visibility = "1" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="LockUnlockSection" Command="LockUnlockSectionHdr" />
          <CATCmdStarter Name = "Section"               Command = "section2CmdHdr" />
        </CATCmdContainer>
        <CATCmdContainer Name = "Print" Visibility = "1" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name = "DMUCaptureCmdStr"     Command = "DMUCaptureCmdHdr" />
          <CATCmdStarter Name = "ViewerScreenshot"     Command = "ViewerScreenshot" />
          <CATCmdStarter Name = "PAD3DViewerPrintCmd"  Command = "PAD3DViewerPrintHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="DMUPrintStr" Command="DMUPrintHdr" />
        <CATCmdSeparator Name = "Markup_Separator"/>
        <CATCmdStarter Name = "CATWebUXPreferencesStr"                 Command = "CATWebUXPreferencesHdr" />
      </CATCmdContainer>

    </CATCmdWorkbench>
  </Template>
</Styles>
