{
    "fetchCapableResource": {
        "url": "/cvservlet/navigate",
        "method": "POST",
        "payload": {
            "label": "Navigate",
            "input_physical_ids": [],
            "version": 3,
            "primitives": [
                {
                    "navigate_to_sr": {
                        "id": 1,
                        "filter": {
                            "role": [
                                "490"
                            ]
                        },
                        "mode": "path"
                    }
                }
            ],
            "patterns": {
                "resourceArea_pathSR": [
                    {
                        "id": 1
                    }
                ]
            },
            "attributes": [
                "ds6w:label",
                "physicalid",
                "ds6w:type",
                "pathsr"
            ]
        }
    },
    "getContextScope": {
        "url": "/resources/v1/modeler/dsprcs/invoke/dsprcs:getScopeContext",
        "method": "POST",
        "payload": {
            "refid": "",
            "stopDAG": "MBOM",
            "firstParentScope": false
        }
    },
    "getMfgItemInstallDependencies": {
        "url": "/resources/v1/modeler/dsmfg/invoke/dsmfg:getMfgItemInstallDependencies",
        "method": "POST",
        "payload": {
            "assemblyid": "",
            "installinstids": [],
            "predecessor": true,
            "successor": false,
            "nodependency": false,
            "graphconstraint": false
        }
    },
    "getProgressiveExpand": {
        "url": "/cvservlet/progressiveexpand/v2?output_format=cvjson",
        "method": "POST",
        "payload": {
            "batch": {
                "expands": [
                    {
                        "label": "ProgressiveExpand_Expand",
                        "root": {
                            "physical_id": ""
                        },
                        "filter": {
                            "all": 1
                        }
                    }
                ]
            },
            "outputs": {
                "select_object": [
                    "physicalid",
                    "ds6w:label"
                ],
                "hits": {
                    "predefined_computation": [
                        "boundingbox"
                    ]
                }
            }
        }
    },
    "getFilteredRep": {
        "payload": {
            "input_physical_ids": [],
            "primitives": [
                {
                    "navigate_from_sr": {
                        "id": 1,
                        "filter": {
                            "role": [
                                "591"
                            ]
                        }
                    }
                }
            ],
            "patterns": {
                "Connection": [
                    {
                        "id": 1
                    }
                ]
            },
            "attributes": [
                "pathsr"
            ],
            "label": "Navigate_Rep_Connections_From_Process",
            "version": 3.0
        }
    },
    "searchUnits": {
        "url": "/resources/v1/modeler/dspfl/dspfl:Unit/search?$mask=dsmvpfl:UnitBaseMask",
        "dataType": "json",
        "method": "GET"
    }
}
