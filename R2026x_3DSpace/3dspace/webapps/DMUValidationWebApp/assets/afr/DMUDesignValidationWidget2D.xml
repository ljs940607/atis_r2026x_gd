  <Template syp:name="3DDesignReviewAfrWorkshopAccess" Target="CATCmdWorkbench">
    <CATCmdWorkbench Name="3DDesignReviewWorkbench">
      <CATCmdContainer Name="DMUFixedArea" RepresentationType="AfrActionBarFixedSection">
        <CATCmdStarter Name="CATWebUXSlideShowHdrStr" Command="CATWebUXSlideShowHdr" />
        <CATCmdStarter Name="DMUCutStr" Command="DMUCutHdr" />
        <CATCmdStarter Name="DMUCopyStr" Command="DMUCopyHdr" />
        <CATCmdContainer Name="PasteContainer" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="DMUPasteStr" Command="DMUPasteHdr" />
          <CATCmdStarter Name="DMUPasteSpecialStr" Command="DMUPasteSpecialHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="ENOR3DHomeStr" Command="Home"/>
      </CATCmdContainer>

      <CATCmdContainer Name="DMUValidation" Visibility="1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="DMUReviewStr" Command="DMUReviewHdr" />
        <CATCmdSeparator Name="CAT3DAnnotSeparator" />
        <CATCmdStarter Name="DMUReviewMarkupStr" Command="DMUReviewMarkupHdr" />
        <CATCmdStarter Name="DMUAddCheckStr" Command="DMUAddCheckHdr" />
        <CATCmdStarter Name="DMUAddHighlightStr" Command="DMUAddHighlightHdr" />
        <CATCmdSeparator Name="CAT3DAnnotSeparator" />
        <CATCmdContainer Name="CATwebIssueIcb" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="CATWebUXGenerateIssueStr" Command="CATWebUXGenerateIssueHdr" />
          <CATCmdStarter Name="CATWebUXGenerateIssueTemplateStr" Command="CATWebUXGenerateIssueTemplateHdr" />
        </CATCmdContainer>
		<CATCmdStarter Name="CATWebUXGenerateTaskStr" Command="CATWebUXGenerateTaskHdr" />
        <CATCmdStarter Name="CATWebUXGenerateCAStr" Command="CATWebUXGenerateCAHdr" />
      </CATCmdContainer>

      <CATCmdContainer Name="DMUMarkup" Visibility="1" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="Slide" Command="slideCmdHdr" />
        <CATCmdStarter Name="DMUEditFormatsStr" Command="DMUEditFormatsHdr"/>
        <CATCmdSeparator Name="CAT3DAnnotSeparator" />

        <CATCmdStarter Name="MarkerArrow" Command="markerArrowCmdHdr" />

        <CATCmdContainer Name="Marker3D" Visibility="1" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="MarkerCircle" Command="markerCircleCmdHdr" />
          <CATCmdStarter Name="MarkerRectangle" Command="DMU3DRectangleCmdHdr" />
          <CATCmdStarter Name="MarkerCloud"      Command="DMUMarkerCloudCreateHdr" />
          <CATCmdStarter Name="MarkerFreehand" Command="DMUFreehandCmdHdr" />
        </CATCmdContainer>

        <CATCmdContainer Name="3DNotes" Visibility="1" RepresentationType="AfrActionBarDropDown">
          <CATCmdStarter Name="MarkerText" Command="markerTextCmdHdr" />
          <CATCmdStarter Name="MarkerPicture" Command="markerPictureCmdHdr" />
          <CATCmdStarter Name="Marker2DText" Command="marker2DTextCmdHdr" />
          <CATCmdStarter Name="Marker2DPicture" Command="marker2DPictureCmdHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="Compare" Command="DMU2DCompareCmdHdr"/>
        <CATCmdStarter Name="Measure" Command="measure2DCmdHdr"/>

        <CATCmdSeparator Name="SeparatorStr" />

        <CATCmdStarter Name="ValidatedHdr" Command="DMUCreateValidatedMkrHdr" />
        <CATCmdStarter Name="RejectedHdr" Command="DMUCreateRejectedMkrHdr" />
      </CATCmdContainer>

      <CATCmdContainer Name="DMUView" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="LeftSidePanelStr" Command="RootPanel"/>
        <CATCmdSeparator Name="ViewSepLeft"/>
        <CATCmdStarter Name="DMUSwitch3DMarkersResponsiveDisplayStr" Command="DMUSwitch3DMarkersResponsiveDisplayHdr" />
        <CATCmdStarter Command="VisuReframeView" Name="PADPADReframeCmd"/>
        <CATCmdStarter Command="VisuPanView" Name="PADPanCmd"/>
        <CATCmdStarter Command="VisuZoomView" Name="PADZoomCmd"/>
        <CATCmdStarter Command="VisuVisibleSpace" Name="VisuVisibleSpaceCmd"/>
        <CATCmdSeparator Name="PadViews_Separator"/>
        <CATCmdStarter Command="CleanAll" Name="PADRemoveAll"/>
        <CATCmdSeparator Name="ViewSepRight" />
        <CATCmdStarter Name="RightSidePanelStr" Command="RightSidePanel" />
      </CATCmdContainer>

      <CATCmdContainer Name="DMUTools" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Command="ActionBar_Attributes" Name="ActionBar_AttributesCmd"/>
        <CATCmdStarter Command="PAD3DViewerRelationalNavigatorHdr" Name="PAD3DViewerRelationalNavigatorCmd"/>
        <CATCmdStarter Name="DMUExportMarkupAsImageStr" Command="DMUExportMarkupAsImageHdr" />
        <CATCmdContainer Name = "Print" Visibility = "1" RepresentationType="AfrActionBarDropDown" >
          <CATCmdStarter Name = "DMUCaptureCmdStr"     Command = "DMUCaptureCmdHdr" /> 
          <CATCmdStarter Name = "ViewerScreenshot"     Command = "ViewerScreenshot" />
          <CATCmdStarter Name = "PAD3DViewerPrintCmd"  Command = "PAD3DViewerPrintHdr" />
        </CATCmdContainer>
        <CATCmdStarter Name="DMUPrintStr" Command="DMUPrintHdr" />
        <CATCmdSeparator Name="ToolsSeparator"/>
        <CATCmdStarter Name="CATWebUXPreferencesStr" Command="CATWebUXPreferencesHdr" />
