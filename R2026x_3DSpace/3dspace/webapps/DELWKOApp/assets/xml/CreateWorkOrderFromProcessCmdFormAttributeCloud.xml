<AttributesDescriptionList Title="Attributes Description List by Types" Description="Sample configuration file delivered by DS." xmlns="DS_DELPPWConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="DS_DELPPWConfiguration ../xsd/CreateWorkOrderFromProcessCmdFormAttributeCloud.xsd">
	<Type name="DELLmiWorkOrderReference">
		<Section name="Section.Attributes">
			<Attribute name="V_Name" dicoType="PLMEntity" disabled="true" displayType="customtext" implementation="DS/DELWKOComponentsUI/DELWKOAutoGenerateTitle" />
      <Attribute name="ProductionPlan" dicoType="DELLmiProductionPlanReference" displayType="customsearch" mandatory="true" defaultValue="resources/v1/modeler/dswo/productionplan" listenCSOEvents="false" serviceName="ODM" implementation="DS/DELWKOComponentsUI/DELWKOProductionPlanAutoCompleteSearchUI"  />
			<Attribute name="MfgProcess" dicoType="DELLmiWorkPlanSystemReference" displayType="customsearch" mandatory="true" defaultValue="resources/v1/modeler/dsprcs/dsprcs:MfgProcess" precondition="((current:RELEASED AND [bo.PLMReference.V_hasConfigContext]:FALSE) OR ([bo.PLMReference.V_hasConfigContext]:TRUE))" serviceName="3DSpace" implementation="DS/DELWKOComponentsUI/DELWKOMfgProcessAutoCompleteSearchUI" />
			<Attribute name="MfgItem" dicoType="DELFmiFunctionReference" displayType="customsearch" defaultValue="resources/v1/modeler/dsmfg/dsmfg:MfgItem" serviceName="3DSpace" implementation="DS/DELWKOComponentsUI/DELWKOMfgItemAutoCompleteSearchUI" />
			<Attribute name="Builds" dicoType="Builds" displayType="customsearch"  defaultValue="" disabled="true" precondition="AND current:Build AND physicalid:" populateCSOEvents="true" implementation="DS/DELWKOComponentsUI/DELWKOBuildAutoCompleteSearchUI" />
			<Attribute name="OrderQuantity" displayType="richText" mandatory="true" />
			<Attribute name="Magnitude" defaultValue="EACH,VOLUME,LENGTH,MASS,AREA" displayType="customselect" implementation="DS/DELWKOComponentsUI/DELWKOMagnitudeCombo" mandatory="true" disabled="true" />
			<Attribute name="UoM" displayType="customselect" disabled="true" implementation="DS/DELWKOComponentsUI/DELWKOUoMCombo" />
			<Attribute name="V_DueDate" dicoType="DELLmiUPRSystemReference" mandatory="true" displayType="DateTime"  defaultValue="14" />
		</Section>
	</Type>
</AttributesDescriptionList>
